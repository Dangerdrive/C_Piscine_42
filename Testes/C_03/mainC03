MAINS03

#########################EX00#######################################
#include <string.h>
#include <stdio.h>

int	ft_strcmp(char *s1, char *s2);

int main() {
	char *str0 = "";
	printf("std: %i, yours: %i\n", strcmp(str0, "a"), ft_strcmp(str0, "a"));

	char *str1 = (char *)"abcadef";
	char *str2 = (char *)"abcadef";
	printf("std: %i, yours: %i\n", strcmp(str1, str2), ft_strcmp(str1, str2));

	str1 = "abcadef";
	str2 = "abcad";
	printf("std: %i, yours: %i\n", strcmp(str1, str2), ft_strcmp(str1, str2));

	str1 = "abcad";
	str2 = "abcadtg";
	printf("std: %i, yours: %i\n", strcmp(str1, str2), ft_strcmp(str1, str2));
}
/////comparar com #include <string.h>
#######################EX01##########################################

#include <stdio.h>

int main()
{
    char str1[5] = "Hello";
    char str2[9] = "Helloooo";
    unsigned int n = 20;

    int result = ft_strncmp(str1, str2, n);

    if (result == 0)
        printf("The strings are equal\n");
    else if (result < 0)
        printf("The first string is less than the second string\n");
    else
        printf("The first string is greater than the second string\n");

    return 0;
}

#####################EX02################################
#include <stdio.h>
#include <string.h>

char *ft_strcat(char *dest, char *src);

int main()
{
    char dest[20] = "Hello";
    char src[] = " World!";
    
    printf("Before: %s\n", dest);
    ft_strcat(dest, src);
    printf("After: %s\n", dest);
    
    return 0;
}
######################EX03##################################
#include <stdio.h>
#include <string.h>

char *ft_strcat(char *dest, char *src);

int main()
{
    char dest[20] = "Hello";
    char src[] = " World!";
    unsigned int n = 10;
    printf("Before: %s\n", dest);
    ft_strncat(dest, src, n);
    printf("After: %s\n", dest);
    
    return 0;
}
